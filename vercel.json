{
  "version": 2,
  "builds": [
    {
      "src": "frontend/weatherly-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/weatherly-app"
    }
  ]
}